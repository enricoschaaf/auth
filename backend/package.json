{
  "name": "auth-backend",
  "version": "0.1.0",
  "bin": {
    "backend": "bin/backend.js"
  },
  "scripts": {
    "synth": "cdk synth --no-staging > template.yaml"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.59",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "10.17.5",
    "@types/source-map-support": "^0.5.2",
    "aws-cdk": "1.54.0",
    "aws-sdk": "^2.717.0",
    "typescript": "~3.7.2"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2": "^1.54.0",
    "@aws-cdk/aws-dynamodb": "^1.54.0",
    "@aws-cdk/aws-iam": "^1.54.0",
    "@aws-cdk/aws-lambda-nodejs": "^1.54.0",
    "@aws-cdk/core": "^1.54.0",
    "jsonwebtoken": "^8.5.0",
    "nanoid": "^3.1.10",
    "source-map-support": "^0.5.16"
  },
  "cdk-lambda": "/asset-output/index.js",
  "targets": {
    "cdk-lambda": {
      "context": "node",
      "includeNodeModules": {
        "aws-sdk": false
      },
      "sourceMap": false,
      "minify": false,
      "engines": {
        "node": ">= 12"
      }
    }
  }
}
